<%- include layouts/header.ejs %>

  <div class="container">
  
    <div class="row">
      <% for(var guitar=0;guitar<Guitar.length; guitar++) { %>
        <div class="col-sm-3">
          <div class="card">
            <div class="card-body">
              <h5 class="card-title"> <%= Guitar[guitar].title %> </h5>
              <img height="300" src="<%= Guitar[guitar].image%>" class="card-img-top" alt="...">
              <p class="card-text"><%= Guitar[guitar].description %> </p>
              <p class="card-text"> Price: $ <%= Guitar[guitar].precie %></p>
              <a href="#" class="btn btn-primary">Buy</a>
            </div>
          </div>
        </div>
      <% } %>
        <div class="col-sm-6">
          <div class="card">
            <div class="card-body">
              <h5 class="card-title">Special title treatment</h5>
              <p class="card-text">With supporting text below as a natural lead-in to additional content.</p>
              <a href="#" class="btn btn-primary">Go somewhere</a>
            </div>
          </div>
        </div>
      </div>
        
    </div>
    
  
  </div>
  <%- include layouts/footer.ejs %>

   